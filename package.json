{
  "name": "Chromo",
  "main": "./lib/color-preview",
  "version": "1.5.0",
  "description": "Displays color previews for hex, rgb, and ansi codes in the gutter as colored pills.",
  "keywords": [
    "Chromo",
    "chromo",
    "hex",
    "color",
    "preview",
    "gutter",
    "colors",
    "rgb",
    "rgba",
    "ansi"
  ],
  "activationCommands": {
    "atom-workspace": "color-preview:toggle"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "repository": "https://github.com/Vertagon-Softworks/Chromo.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {}
}
