{
  "name": "Chromo - Color Previews",
  "main": "./lib/color-preview",
  "version": "1.6.3",
  "description": "Displays color previews for hex, rgb(a), hsl(a), named colors, gradients, and all 255 ansi color codes in the gutter as colored pills.",
  "keywords": [
    "chromo",
    "hex",
    "color",
    "preview",
    "gutter",
    "rgb",
    "ansi",
    "gradient",
    "pills"
  ],
  "activationCommands": {
    "atom-workspace": "color-preview:toggle"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "repository": "https://github.com/Vertagon-Softworks/Chromo.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {}
}
